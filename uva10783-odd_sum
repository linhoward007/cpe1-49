#include <bits/stdc++.h>
using namespace std;
int main()
{
    
    int numbers,ct=0;
    cin>>numbers;
    while(numbers--)
    {
        int a,b;
        cin>>a>>b;
        int sum=0;
        for(int i=a;i<=b;i++)
        {
            if(i%2 == 1)
                sum += i;
        }
        cout<<"Case "<<++ct<<": "<<sum<<endl;
    }
    
    return 0;
}
